{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint/eslint-plugin"
  ],
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "ignorePatterns": [
    "*/dist/**.{ts,js}"
  ],
  "rules": {
    "no-console": "warn",
    "semi": "error",
    "@typescript-eslint/no-explicit-any": "off"
  }
}